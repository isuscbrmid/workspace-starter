{
  "devtools_port": 9090,
  "licenseKey": "openfin-demo-license-key",
  "runtime": {
    "arguments": "--v=1 --inspect",
    "version": "23.96.67.7"
  },
  "platform": {
    "uuid": "dowjones-openfin-poc",
    "icon": "https://newsplus.int.wsj.com/public/favicon.ico",
    "autoShow": false,
    "providerUrl": "https://openfin.web.int.pib.dowjones.io/provider.html",
    "preventQuitOnLastWindowClosed": true,
    "permissions": {
      "System": {
        "launchExternalProcess": true,
        "openUrlWithBrowser": {
          "enabled": true,
          "protocols": [
            "mailto"
          ]
        }
      }
    },
    "defaultWindowOptions": {
      "permissions": {
        "System": {
          "openUrlWithBrowser": {
            "enabled": true,
            "protocols": [
              "mailto"
            ]
          }
        }
      }
    }
  },
  "snapshot": {
    "windows": []
  },
  "shortcut": {
    "company": "Dowjones",
    "description": "An way of showing examples of what OpenFin can do.",
    "icon": "https://newsplus.int.wsj.com/public/favicon.ico",
    "name": "Dowjones Headlines POC",
    "target": [
      "desktop",
      "start-menu"
    ]
  },
  "customSettings": {
    "appProvider": {
      "apps": [
        {
          "appId": "expero-stock-quote",
          "name": "expero-stock-quote",
          "title": "Gateway - Stock Quote",
          "manifest": "https://openfin-iex.experolabs.com/openfin/manifests/stock-quote.json",
          "manifestType": "view",
          "icons": [
            {
              "src": "https://openfin-iex.experolabs.com/favicon.ico"
            }
          ],
          "contactEmail": "contact@example.com",
          "supportEmail": "support@example.com",
          "publisher": "Expero",
          "intents": [],
          "images": [{
            "src":"http://localhost:8080/images/previews/expero-stock-view.png"
          }],
          "tags":["expero", "view", "interop"]
        },
        {
          "appId": "dowjones-headlines",
          "name": "Dowjones Headlines",
          "title": "Dowjones Headlines",
          "description": "Dowjones headlines openfin integration",
          "manifest": {
            "url": "https://newsplus.int.wsj.com/search/realtime/company/?searchFilterState=open&openfin=true",
            "interop": {
              "contextDeclaration": [
                "instrument"
              ]
            }
          },
          "manifestType": "direct",
          "icons": [
            { "src": "https://newsplus.int.wsj.com/public/favicon.ico" }
          ],
          "contactEmail": "jesus.cabrera@dowjones.com",
          "supportEmail": "jesus.cabrera@dowjones.com",
          "publisher": "Dowjones",
          "intents": [],
          "images": [],
          "tags":["view", "interop", "dowjones"]
        },
        {
          "appId": "expero-description",
          "name": "expero-description",
          "title": "Gateway - Description",
          "manifest": "https://openfin-iex.experolabs.com/openfin/manifests/description.json",
          "manifestType": "view",
          "icons": [
            {
              "src": "https://openfin-iex.experolabs.com/favicon.ico"
            }
          ],
          "contactEmail": "contact@example.com",
          "supportEmail": "support@example.com",
          "publisher": "Expero",
          "intents": [],
          "images": [{
            "src":"http://localhost:8080/images/previews/expero-description-view.png"
          }],
          "tags":["expero", "view", "interop"]
        }
      ],
      "includeCredentialOnSourceRequest": "include"
    },
    "homeProvider": {
      "id": "register-with-home",
      "title": "Headlines POC",
      "icon": "https://newsplus.int.wsj.com/public/favicon.ico",
      "queryMinLength": 3,
      "queryAgainst": [
        "title"
      ]
    }
  }
}